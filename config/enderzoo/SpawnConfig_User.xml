  <entry id="ConcussionCreeper" mobName="concussioncreeper" creatureType="MONSTER" rate="40" minGroupSize="1" maxGroupSize="3" remove="false">
    <biomeFilter type="any">
      <biome type="BASE_LAND_TYPES" />
      <biome type="NETHER" exclude="true" />
      <biome type="END" exclude="true" />
      <biome type="MUSHROOM" exclude="true" />
    </biomeFilter>   
    <dimensionExclude name="Twilight Forest"/>           
    <dimensionExclude name="Dungeon"/>
	<dimensionExclude idStart="-1600" idEnd="-2"/>
	<dimensionExclude idStart="2" idEnd="1600"/>
  </entry>

  <entry id="EnderMiny" mobName="enderminy" rate="30">
    <biomeFilter type="any">
      <biome type="FOREST" />
      <biome type="SWAMP" />
      <biome type="JUNGLE" />
      <biome type="NETHER" exclude="true" />
      <biome type="END" exclude="true" />
    </biomeFilter>
    <dimensionExclude name="Twilight Forest"/>
	<dimensionExclude idStart="-1600" idEnd="-2"/>
	<dimensionExclude idStart="2" idEnd="1600"/>
  </entry>

  <entry id="FallenKnight" mobName="fallenknight" rate="30" minGroupSize="1" maxGroupSize="4">
    <biomeFilter type="any">
      <biome type="PLAINS" />
      <biome type="NETHER" exclude="true" />
      <biome type="END" exclude="true" />
    </biomeFilter>
    <dimensionExclude name="Twilight Forest"/>
	<dimensionExclude idStart="-1600" idEnd="-2"/>
	<dimensionExclude idStart="2" idEnd="1600"/>
  </entry>

  <entry id="WitherWitch" mobName="witherwitch" rate="5" minGroupSize="1" maxGroupSize="1">
    <biomeFilter type="any">
      <biome type="FOREST" />
      <biome type="SWAMP" />
      <biome type="PLAINS" />
      <biome type="HILLS" />
      <biome type="MOUNTAIN" />
      <biome type="NETHER" exclude="true" />
      <biome type="END" exclude="true" />
    </biomeFilter>
    <dimensionExclude name="Twilight Forest"/>
	<dimensionExclude idStart="-1600" idEnd="-2"/>
	<dimensionExclude idStart="2" idEnd="1600"/>
