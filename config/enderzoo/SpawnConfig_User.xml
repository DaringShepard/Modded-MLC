  <entry id="DireWolf" mobName="direwolf" rate="30" minGroupSize="2" maxGroupSize="6">
    <biomeFilter type="all">
      <biome type="SNOWY" />
      <biome type="CONIFEROUS" />      
      <biome type="NETHER" exclude="true" />
      <biome type="END" exclude="true" />
    </biomeFilter>
	<dimensionExclude idStart="-1600" idEnd="-2"/>
	<dimensionExclude idStart="2" idEnd="1600"/>
  </entry>
  
   <entry id="Owl" mobName="owl" rate="5" minGroupSize="1" maxGroupSize="1" creatureType="CREATURE">
