    <biomeFilter type="any">
      <biome type="BASE_LAND_TYPES" />
    </biomeFilter>
  </entry>
 --> 

<!-- Enabling this section will remove the default wither witch spawn config and replace it with
     a new config with a higher spawn rate
 <entry id="WitherWitch" mobName="witherwitch" rate="5" remove="true">
    <biomeFilter type="any">
      <biome type="FOREST" />
      <biome type="SWAMP" />
      <biome type="PLAINS" />
      <biome type="HILLS" />
      <biome type="MOUNTAIN" />
      <biome type="NETHER" exclude="true" />
      <biome type="END" exclude="true" />
    </biomeFilter>
    <dimensionExclude name="Twilight Forest"/>
  </entry>

   <entry id="WitherWitch" mobName="witherwitch" rate="20" minGroupSize="1" maxGroupSize="1">
    <biomeFilter type="any">
      <biome type="FOREST" />
      <biome type="SWAMP" />
      <biome type="PLAINS" />
      <biome type="HILLS" />
      <biome type="MOUNTAIN" />
      <biome type="NETHER" exclude="true" />
      <biome type="END" exclude="true" />
